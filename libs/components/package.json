  "peerDependencies": {
    "@proxym/themes": "^0.0.6",
    "polished": "^4.2.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-native": "^0.69.7",
    "react-native-web": "^0.18.10"
  },
  "react-native": "src/index.ts",
  "types": "lib/typescript/src/index.d.ts",
